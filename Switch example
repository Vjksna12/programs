using System;
namespace switchExample
{
    class example
    {
        static void Main()
        {
            Console.WriteLine("enter the number");
            int num =int.Parse(Console.ReadLine());
            switch(num)
            {
                case 1: 
                Console.WriteLine("number is one");
                break;
                case 2:
                Console.WriteLine("number is two");
                break;
                default:
                Console.WriteLine("not between one and two");
                break;
            }

        }
    }
}
