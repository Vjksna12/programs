using System;
namespace forandForEachLOOp
{
    class example
    {
        static void Main()
        {
            int[] arr=new int[3];
            arr[1]=23;
            arr[2]=45;
            arr[0]=34;
            for(int i=0;i<arr.Length;i++)
            {
                Console.WriteLine(arr[i]);
            }
            foreach(int i in arr)
            {
                Console.WriteLine(i);
            }
        } 

          
    }
}
