using System;
namespace nullableValues
{
    class example
    {
        static void Main()
        {
            bool? b = false;
            if(b==true)
            {
                Console.WriteLine("true");
            }
            else if(b==false)
            {
                Console.WriteLine("false");
            
            }
            else{
                Console.WriteLine("it is a null value");
            }
            bool c = b ?? true;
            Console.WriteLine("nullcoalecing {0}",c);

        }
    }
}
